{
  "name": "api-aone",
  "version": "0.1.0",
  "description": "Aone API Service",
  "main": "index.js",
  "repository": "https://github.com/bhaktibuana/backend-aone.git",
  "author": "Bhakti Mega Buana <bhaktibuana19@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "start": "cross-env NODE_ENV=production node -r tsconfig-paths/register ./dist/index.js",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "seed:generate": "sequelize-cli seed:generate --name",
    "migrate:dev": "tsc && cross-env NODE_ENV=development sequelize-cli db:migrate",
    "seed:dev": "tsc && cross-env NODE_ENV=development node -r tsconfig-paths/register ./database/seeders",
    "migrate:prod": "tsc && cross-env NODE_ENV=production sequelize-cli db:migrate",
    "seed:prod": "tsc && cross-env NODE_ENV=production node -r tsconfig-paths/register ./database/seeders"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/nodemailer": "^6.4.14",
    "@types/otp-generator": "^4.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "sequelize-cli": "^6.6.2",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "device-detector-js": "^3.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.3",
    "nodemailer": "^6.9.7",
    "otp-generator": "^4.0.1",
    "sequelize": "^6.35.0"
  }
}
