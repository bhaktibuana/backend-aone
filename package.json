{
  "name": "api-aone",
  "version": "0.1.0",
  "description": "Aone API Service",
  "main": "index.js",
  "repository": "https://github.com/bhaktibuana/backend-aone.git",
  "author": "Bhakti Mega Buana <bhaktibuana19@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only ./src/index.ts",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "migrate:generate": "sequelize-cli migration:generate --name",
    "seed:generate": "sequelize-cli seed:generate --name",
    "migrate:dev": "tsc && cross-env NODE_ENV=development sequelize-cli db:migrate",
    "seed:dev": "tsc && cross-env NODE_ENV=development node ./database/seeders",
    "migrate:prod": "tsc && cross-env NODE_ENV=production sequelize-cli db:migrate",
    "seed:prod": "tsc && cross-env NODE_ENV=production node ./database/seeders"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "sequelize-cli": "^6.6.2",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "mysql2": "^3.6.3",
    "sequelize": "^6.35.0"
  }
}
